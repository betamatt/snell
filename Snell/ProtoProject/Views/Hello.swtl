<html>
  <body>
    <h1>Gee, this is Snell.</h1>
    <div>
      <h2>Request Params</h2>
      <% for i in scope.request.params.keys { %>
        <li><%= i %> = <%= scope.request.params[i]! %>
      <% } %>
    </div>

    <div>
      <h2>Environment</h2>
      <% for i in scope.environment.keys { %>
        <li><%= i %> = <%= scope.environment[i]! %>
      <% } %>
    </div>
  </body>
</html>